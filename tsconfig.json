{
  "extends": "astro/tsconfigs/strictest",
  "include": ["src/**/*", "*.cjs", "*.ts", ".eslintrc.cjs"],
  "compilerOptions": {
    "module": "ES2022",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@comp/*": ["src/components/content/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["unplugin-icons/types/solid"]
  }
}
