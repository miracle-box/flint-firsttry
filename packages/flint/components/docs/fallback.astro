---
import Callout from '../content/callout.astro';

type Props = {
	fallbackLocale: string;
};

const { fallbackLocale } = Astro.props;

const { locale } = Astro.locals;
const t = Astro.locals.translations;
---

<Callout type="warning" title={false}>
	{t.docs.fallbackTip(locale, fallbackLocale)}
</Callout>
