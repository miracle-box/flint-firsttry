{
  "name": "flint",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.10",
  "scripts": {
    "check": "npm-run-all --silent check:prettier --parallel check:tsc check:eslint",
    "fix:prettier": "prettier . --write --ignore-path ./.gitignore --plugin-search-dir=.",
    "fix:eslint": "eslint . --fix --ignore-path ./.gitignore",
    "check:prettier": "prettier . --check --loglevel warn --ignore-path ./.gitignore --plugin-search-dir=.",
    "check:tsc": "tsc --noEmit",
    "check:eslint": "eslint . --ignore-path ./.gitignore"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/solid-js": "^2.2.0",
    "@astrojs/tailwind": "^4.0.0",
    "@fontsource-variable/inter": "^5.0.4",
    "@fontsource/fira-mono": "^5.0.4",
    "@fontsource/noto-sans-sc": "^5.0.4",
    "@iconify-json/octicon": "^1.1.44",
    "astro": "^2.7.3",
    "github-markdown-css": "^5.2.0",
    "just-extend": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "solid-headless": "^0.13.1",
    "solid-js": "^1.7.7",
    "tailwindcss": "^3.3.2",
    "unplugin-icons": "^0.16.3"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.57.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.12.1",
    "prettier": "2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "typescript": "^5.1.6",
    "xo": "^0.54.2"
  }
}
